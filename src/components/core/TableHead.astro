---
export interface Props {
	cols: object;
}

const { cols } = Astro.props;
---
<thead class="dark:bg-slate-700 bg-slate-200 text-slate-900 dark:text-white">
    <tr class="text-lg font-semibold tracking-wide text-left uppercase border">
        {cols.map((item) => (
            <th class="px-4 py-4">{item}</th>
        ))}
    </tr>
</thead>